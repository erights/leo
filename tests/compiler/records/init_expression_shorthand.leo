record Token {
    // The token owner.
    owner: address,
    // The Aleo balance (in gates).
    balance: u64,
    // The token amount.
    amount: u64,
}

function mint(owner: address, amount: u64) -> Token {
    return Token {
        owner,
        balance: 0u64,
        amount,
    };
}

function main(x: address) -> u64 {
    const c: u64 = 1u64;
    let t: Token = mint(x, c);

    return t.balance;
}